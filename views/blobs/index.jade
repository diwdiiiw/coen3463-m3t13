doctype html
html
  head
    title= title
    link(href='css/css.css', rel='stylesheet')
  body
    h1
      span.yellow Philippine State Schools, Colleges and Universities
    h2
      | Go Back 
      a(href='/home') HOME
    h2
      | Total Number of Schools:
      span#totalCount

    h2
      | Search our Database:
      form(method= 'post', action= '../blobs/', onsubmit= 'getSearch()')
        input#search(type='search', placeholder="Search School")
    
    table.container
      thead
        tr
          th
            h1 NAME
          th
            h1 LOCALE
          th
            h1 ESTABLISHED
          th
            h1 DATE CREATED
      tbody#table-body


      script(type="text/javascript", src="js/main.js")

      
       
                